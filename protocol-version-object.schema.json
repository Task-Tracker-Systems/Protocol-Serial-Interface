{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Protocol Version Object",
  "properties": {
    "major": {
      "type": "integer",
      "minimum": 0
    },
    "minor": {
      "type": "integer",
      "minimum": 0
    },
    "patch": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": ["major", "minor", "patch"]
}

